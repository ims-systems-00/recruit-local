{
  "name": "@rl/backend",
  "version": "1.0.0",
  "description": "Interface NRM portal backend api",
  "main": "server.js",
  "scripts": {
    "bulk-import": "env-cmd -f ./.env.dev ts-node scripts/bulk-import.ts",
    "generate-cert": "ts-node src/common/helper/generate-certificate.ts",
    "clean": "rimraf -rf dist/",
    "start": "node dist/src/server.js",
    "build": "pnpm clean && npx tsc",
    "postbuild": "ts-node scripts/postEsBuild.ts",
    "start:dev": "env-cmd -f ./.env.dev nodemon",
    "start:docker": "docker-compose -f ../docker-compose.dev.yml up -d --build",
    "stop:docker": "docker compose -f ../docker-compose.dev.yml down",
    "seed:dev": "env-cmd -f ./.env.dev ts-node ./src/seeders/index.ts",
    "seed": "node dist/src/seeders/index.js",
    "migrate:dev": "env-cmd -f ./.env.dev migrate-mongo up",
    "migrate-down:dev": "env-cmd -f ./.env.dev migrate-mongo down",
    "migrate": "migrate-mongo"
  },
  "repository": "https://github.com/ims-systems-00/interface-nrm-portal",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.682.0",
    "@aws-sdk/s3-request-presigner": "^3.682.0",
    "@casl/ability": "^6.7.3",
    "@casl/mongoose": "^8.0.3",
    "@ims-systems-00/ims-query-builder": "^0.0.11",
    "@rl/authz": "workspace:*",
    "@rl/types": "workspace:*",
    "@rl/utils": "workspace:*",
    "@riadhossain43/mongoose-autoincrement": "^2.1.1",
    "@sendgrid/mail": "^8.1.4",
    "@types/string-similarity": "^4.0.2",
    "agenda": "^5.0.0",
    "angular-expressions": "^1.4.3",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.0",
    "cheerio": "^1.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "docxtemplater": "^3.63.2",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.3.0",
    "imghash": "^1.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "libreoffice-convert": "^1.6.1",
    "lodash": "^4.17.21",
    "migrate-mongo": "^11.0.0",
    "moment": "^2.30.1",
    "mongoose": "^8.5.2",
    "mongoose-aggregate-paginate-v2": "^1.1.2",
    "mongoose-paginate-v2": "^1.8.3",
    "morgan": "^1.10.0",
    "node-geocoder": "^4.4.0",
    "nodemailer": "^6.9.14",
    "pizzip": "^3.2.0",
    "rimraf": "^6.0.1",
    "shelljs": "^0.8.5",
    "string-similarity": "^4.0.4",
    "uuid": "^11.0.2",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.7",
    "@types/mongoose-paginate-v2": "^1.6.5",
